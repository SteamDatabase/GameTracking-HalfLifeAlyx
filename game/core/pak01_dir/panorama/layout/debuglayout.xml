<root>
	<styles>		
		<include src="s2r://panorama/styles/debugger.vcss_c" />
	</styles>

	<snippets>
		<snippet name="PanelStart">
			<Panel class="DebugLabelPanelRow" acceptsfocus="true">
				<Panel class="DebugLayoutPanelPerfDisplay">
					<Panel id="DebugLayoutPanelStylesPerf" class="DebugLayoutPanelPerf">
						<Label text="S:" />
						<Panel class="DebugLayoutPanelPerfBarContainer">
							<Panel id="DebugLayoutPanelStylesPerfBar" />
						</Panel>
					</Panel>
					<Panel id="DebugLayoutPanelLayoutPerf" class="DebugLayoutPanelPerf">
						<Label text="L:" />
						<Panel class="DebugLayoutPanelPerfBarContainer">
							<Panel id="DebugLayoutPanelLayoutPerfBar" />
							<Panel id="DebugLayoutPanelSecondaryLayoutPerfBar" />
						</Panel>
					</Panel>
					<Panel id="DebugLayoutPanelPaintPerf" class="DebugLayoutPanelPerf">
						<Label text="P:" />
						<Panel class="DebugLayoutPanelPerfBarContainer">
							<Panel id="DebugLayoutPanelPaintPerfBar" />
						</Panel>
					</Panel>
					<Panel id="DebugLayoutPanelRenderPerf" class="DebugLayoutPanelPerf">
						<Label text="R:" />
						<Panel class="DebugLayoutPanelPerfBarContainer">
							<Panel id="DebugLayoutPanelRenderPerfBar" />
						</Panel>
					</Panel>
				</Panel>
				<Panel id="Indent">
					<ToggleButton id="DebugLabelToggle" selected="true" />					
				</Panel>
				<Label id="DebugLayoutPanelOpen" acceptsfocus="true" />
				<Label id="StyleLink" class="StyleLink" />
			</Panel>
		</snippet>

		<snippet name="PanelClose">
			<Panel class="DebugLabelPanelRow" acceptsfocus="true">
				<Panel class="DebugLayoutPanelPerfDisplay" />
				<Panel id="Indent" />
				<Label id="DebugLayoutPanelClose" />
			</Panel>
		</snippet>
	</snippets>
	
	<DebugLayout>
		<!-- Filled out with a list of the snippets above -->
	</DebugLayout>	
</root>
