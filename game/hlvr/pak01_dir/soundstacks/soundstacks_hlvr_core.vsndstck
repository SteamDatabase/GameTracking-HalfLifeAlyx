// hlvr_beacon_housekeeping
"hlvr_beacon_housekeeping"
{
	"public"
	{
		"operator"		"util_null"
		"operator_variables"
		{
			"opvar_beacon_name"
			{
				"data_type"		"string"
				"value"		"proximity_beacon"
			}
			"opvar_array_name"
			{
				"data_type"		"string"
				"value"		"proximity_array"
			}
			"stack_name"
			{
				"data_type"		"string"
				"value"		"hlvr_global_opvars"
			}
		}
	}
	"get_val_array"
	{
		"execute_once"		"false"
		"operator"		"opvar_get_float"
		"check_event_data"		"false"
		"input_stack_name"		"@public.stack_name"
		"input_operator_name"		"opvars"
		"input_field_name"		"@public.opvar_array_name"
		"input_use_namespace"		"false"
		"array_selection_type"		"max_value"
	}
	"set_val"
	{
		"operator"		"opvar_set_float"
		"input_execute"		"1.000000"
		"input"		"@get_val_array.output"
		"input_index"		"0"
		"input_stack_name"		"@public.stack_name"
		"input_operator_name"		"opvars"
		"input_field_name"		"@public.opvar_beacon_name"
		"input_use_namespace"		"false"
	}
}

// hlvr_beacon_invert_housekeeping
"hlvr_beacon_invert_housekeeping"
{
	"public"
	{
		"operator"		"util_null"
		"operator_variables"
		{
			"opvar_beacon_name"
			{
				"data_type"		"string"
				"value"		"proximity_beacon"
			}
			"opvar_invert_beacon_name"
			{
				"data_type"		"string"
				"value"		"invert_beacon"
			}
		}
	}
	"get_beacon"
	{
		"operator"		"opvar_get_float"
		"input_execute"		"1.000000"
		"input_stack_name"		"hlvr_global_opvars"
		"input_operator_name"		"opvars"
		"input_field_name"		"@public.opvar_beacon_name"
		"input_use_namespace"		"false"
	}
	"get_invert_beacon"
	{
		"operator"		"opvar_get_float"
		"input_execute"		"1.000000"
		"input_stack_name"		"hlvr_global_opvars"
		"input_operator_name"		"opvars"
		"input_field_name"		"@public.opvar_invert_beacon_name"
		"input_use_namespace"		"false"
	}
	"invert_scalar"
	{
		"operator"		"math_float"
		"apply"		"sub"
		"input1"		"1.000000"
		"input2"		"@get_invert_beacon.output"
	}
	"mult_invert_scalar_and_beacon"
	{
		"operator"		"math_float"
		"apply"		"mult"
		"input1"		"@invert_scalar.output"
		"input2"		"@get_beacon.output"
	}
	"set_beacon"
	{
		"operator"		"opvar_set_float"
		"input_execute"		"1.000000"
		"input"		"@mult_invert_scalar_and_beacon.output"
		"input_index"		"0.000000"
		"input_stack_name"		"hlvr_global_opvars"
		"input_operator_name"		"opvars"
		"input_field_name"		"@public.opvar_beacon_name"
		"input_use_namespace"		"false"
	}
}

