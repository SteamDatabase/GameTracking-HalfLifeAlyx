<root>
	<styles>
		<include src="s2r://panorama/styles/base_styles.vcss_c" />
		<include src="s2r://panorama/styles/personal/jaker/hudtest.vcss_c" />
	</styles>

	<Panel class="SpectatorHUD RightHanded">

		<Panel id="HealthAndResin" class="HUD_Bottom">
			<Panel id="ResinContainer" class="Container_Resin">
				<Label id="ResinLabel" class="TextLabel" text="RESIN" />
				<Label id="ResinCount" class="TextResinCount" text="10" />
			</Panel>
			<Panel id="container_health" class="Container_Health ">
				<Label class="TextLabel" text="HEALTH" />
				<Panel id="HeartContainer" class="Container_Hearts ">
					<Panel id="health_heart_1" class="Icon_Heart health_icon_00" />
					<Panel id="health_heart_2" class="Icon_Heart health_icon_00" />
					<Panel id="health_heart_3" class="Icon_Heart health_icon_00" />
				</Panel>
			</Panel>
		</Panel>
		
		
		<Panel id="GunsAndAmmo" class="HUD_Bottom">
			<Panel id="container_Guns" class="Container_Guns ">
					<Panel id="icon_gun_pistol" class="Icon_Gun_Pistol active" />
					<Panel id="icon_gun_shotgun" class="Icon_Gun_Shotgun" />
					<Panel id="icon_gun_rapid_fire" class="Icon_Gun_Rapid_Fire" />
			</Panel>
			<Panel id="container_Ammo" class="Container_Ammo ">
				<Label class="TextLabel" text="AMM0" />
				<Panel class="Container_Ammo_Counter">
					<Label id="ammoclipcount_10" class="TextAmmoClipCount" text="4" />
					<Label id="ammoclipcount_1" class="TextAmmoClipCount" text="0" />
					<Label class="TextBackpackCount" text="/" />
					<Label class="TextBackpackCount" text="427" />
				</Panel>
			</Panel>
		</Panel>

		<Panel id="WristPouchLeft" class="WristPouch Left">
			<!--In Progress Animations
			<Panel class="WristPouchCircleInverted" />	
			<Panel class="WristPouchCircle" />
			-->
			<Panel id="WristItemLeft" class="WristItem HealthPen" />
		</Panel>

		<Panel id="WristPouchRight" class="WristPouch Right">
			<Panel id="WristItemRight" class="WristItem" />
		</Panel>
		
	</Panel>	
	

</root>


	